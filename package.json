{
  "name": "habit-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "jest || echo 'Tests not implemented yet'",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "vercel-build": "npm install --legacy-peer-deps && npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./reports/lighthouse",
    "analyze": "npm run build && npx @next/bundle-analyzer",
    "clean": "rm -rf .next && rm -rf out && rm -rf dist",
    "quality": "npm run lint && npm run type-check && npm run test",
    "pre-commit": "npm run lint && npm run type-check",
    "prepare": "husky install || echo 'Husky not installed, skipping pre-commit hooks'",
    "release": "standard-version && git push --follow-tags",
    "docker:build": "docker build -t habit-tracker .",
    "docker:run": "docker run -p 3000:3000 habit-tracker",
    "health-check": "npm run build && npm run lint && npm run type-check",
    "quick-start": "npm install --legacy-peer-deps && npm run dev",
    "predeploy": "npm run health-check",
    "deploy:local": "npm run build && npx serve .next"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.2.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.2.2",
    "@tanstack/react-query": "^5.61.0",
    "@tanstack/react-query-devtools": "^5.91.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.462.0",
    "next": "16.1.6",
    "next-auth": "^4.24.10",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.13.3",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.17.10",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/web-push": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.4.49",
    "prisma": "^6.2.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
